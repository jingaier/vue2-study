#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vue/vue2-study/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/opener@1.5.2/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules:/mnt/d/vue/vue2-study/node_modules:/mnt/d/vue/node_modules:/mnt/d/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules/opener/bin/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules/opener/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vue/vue2-study/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/opener@1.5.2/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules:/mnt/d/vue/vue2-study/node_modules:/mnt/d/vue/node_modules:/mnt/d/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules/opener/bin/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules/opener/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../opener/bin/opener-bin.js" "$@"
else
  exec node  "$basedir/../opener/bin/opener-bin.js" "$@"
fi
