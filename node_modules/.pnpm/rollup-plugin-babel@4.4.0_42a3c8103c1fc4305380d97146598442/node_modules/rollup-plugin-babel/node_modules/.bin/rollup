#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules/rollup/dist/bin/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules/rollup/dist/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules/rollup/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules:/mnt/d/vue/vue2-study/node_modules:/mnt/d/vue/node_modules:/mnt/d/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-babel@4.4.0_42a3c8103c1fc4305380d97146598442/node_modules/rollup-plugin-babel/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-babel@4.4.0_42a3c8103c1fc4305380d97146598442/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules/rollup/dist/bin/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules/rollup/dist/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules/rollup/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup@3.28.0/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules:/mnt/d/vue/vue2-study/node_modules:/mnt/d/vue/node_modules:/mnt/d/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-babel@4.4.0_42a3c8103c1fc4305380d97146598442/node_modules/rollup-plugin-babel/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-babel@4.4.0_42a3c8103c1fc4305380d97146598442/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@3.28.0/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@3.28.0/node_modules/rollup/dist/bin/rollup" "$@"
fi
