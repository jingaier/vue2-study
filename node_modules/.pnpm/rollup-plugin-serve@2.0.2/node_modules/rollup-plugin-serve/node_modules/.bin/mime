#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vue/vue2-study/node_modules/.pnpm/mime@3.0.0/node_modules/mime/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/mime@3.0.0/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules:/mnt/d/vue/vue2-study/node_modules:/mnt/d/vue/node_modules:/mnt/d/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-serve@2.0.2/node_modules/rollup-plugin-serve/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-serve@2.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vue/vue2-study/node_modules/.pnpm/mime@3.0.0/node_modules/mime/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/mime@3.0.0/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/node_modules:/mnt/d/vue/vue2-study/node_modules:/mnt/d/vue/node_modules:/mnt/d/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-serve@2.0.2/node_modules/rollup-plugin-serve/node_modules:/mnt/d/vue/vue2-study/node_modules/.pnpm/rollup-plugin-serve@2.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@3.0.0/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@3.0.0/node_modules/mime/cli.js" "$@"
fi
